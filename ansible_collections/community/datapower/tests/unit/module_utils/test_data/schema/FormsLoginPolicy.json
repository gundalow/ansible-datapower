                "uri": "webapp/forms-login-policy",
                "summary": "Policy that defines HTML form behavior",
                "cmd-group": "mpgw",
                "cli-alias": "forms-login-policy",
                "properties": {
                    "property": [
                        {
                            "name": "mAdminState",
                            "default": "enabled",
                            "type": {
                                "href": "/mgmt/types/default/dmAdminState"
                            },
                            "label": "Enable administrative state",
                            "summary": "Set the administrative state of this configuration.",
                            "cli-alias": "admin-state",
                            "display": "Administrative state",
                            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>",
                            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
                        },
                        {
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "display": "Comments",
                            "cli-alias": "summary",
                            "name": "UserSummary",
                            "summary": "Brief summary for user annotation."
                        },
                        {
                            "name": "LoginForm",
                            "default": "/LoginPage.htm",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "URL fragment of login form",
                            "cli-alias": "login-url",
                            "display": "Login",
                            "description": "The URL fragment of the login page on the application server. The login page collects user name and password information. The default is /LoginPage.htm."
                        },
                        {
                            "name": "UseCookieAttributes",
                            "default": "no",
                            "type": {
                                "href": "/mgmt/types/default/dmUseCookieAttributes"
                            },
                            "required": "true",
                            "summary": "Indicate whether to attach a policy that describes cookie attributes.",
                            "cli-alias": "use-cookie-attribute",
                            "display": "Attach Cookie Attribute Policy",
                            "description": "Whether to attach a cookie attribute policy. If you do not use a cookie attribute policy, the configuration use the properties of the forms login policy. The default behavior is not to attach a cookie attribute policy."
                        },
                        {
                            "name": "CookieAttributes",
                            "required-when": {
                                "condition": {
                                    "property-name": "UseCookieAttributes",
                                    "evaluation": "property-equals",
                                    "value": "yes"
                                }
                            },
                            "summary": "Set the policy that describes cookie attributes.",
                            "cli-alias": "cookie-attributes",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/CookieAttributePolicy"
                                }
                            },
                            "display": "Cookie Attributes",
                            "description": "&lt;p>Use a cookie attribute policy to allow predefined or custom attributes to be included in forms login cookie. Properties in the policy override common attributes of the forms login policy. &lt;ul>&lt;li>&lt;tt>Max-Age&lt;/tt> overrides inactivity timeout and session lifetime.&lt;/li>&lt;li>&lt;tt>Path&lt;/tt> replaces the form POST action URL.&lt;/li>&lt;li>&lt;tt>Secure&lt;/tt> replaces the use TLS for login.&lt;/li>&lt;/ul>&lt;/p>"
                        },
                        {
                            "name": "UseSSLForLogin",
                            "default": "on",
                            "required-when": {
                                "condition": {
                                    "property-name": "FormSupportType",
                                    "evaluation": "property-equals",
                                    "value": "static"
                                }
                            },
                            "summary": "Whether to use TLS for the login",
                            "cli-alias": "use-ssl",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "display": "Use TLS for Login",
                            "description": "Whether to use a secure connection for the login exchange. If enabled, you must provide the TLS port. The default is enabled."
                        },
                        {
                            "name": "EnableMigration",
                            "default": "off",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "required": "true",
                            "summary": "Whether to allow sessions to migrate between cluster members if a DataPower Gateway fails",
                            "cli-alias": "enable-migration",
                            "display": "Enable Session Migration",
                            "description": "Whether to permit session failover between cluster members. If enabled, you must specify a shared secret. The default is disabled."
                        },
                        {
                            "name": "SSLPort",
                            "default": 8080,
                            "required-when": {
                                "condition": {
                                    "evaluation": "logical-and",
                                    "condition": [
                                        {
                                            "property-name": "FormSupportType",
                                            "evaluation": "property-equals",
                                            "value": "static"
                                        },
                                        {
                                            "property-name": "UseSSLForLogin",
                                            "evaluation": "property-equals",
                                            "value": "on"
                                        }
                                    ]
                                }
                            },
                            "maximum": 65535,
                            "summary": "Port for TLS login redirection",
                            "cli-alias": "ssl-port",
                            "minimum": 1,
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "TLS Port",
                            "description": "The port the service uses for TLS redirection during the login transaction. Specify the port of the HTTPS handler that the Web application firewall uses for secure communication. The default is 8080."
                        },
                        {
                            "name": "SharedSecret",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/CryptoSSKey"
                                }
                            },
                            "summary": "Shared secret, common to all DataPower Gateways to which a session may migrate",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "EnableMigration",
                                    "evaluation": "property-equals",
                                    "value": "off"
                                }
                            },
                            "cli-alias": "shared-secret",
                            "display": "Shared Secret",
                            "description": "Shared secret object that will be used to protect the migration cookie. All cluster members must specify the same shared secret."
                        },
                        {
                            "name": "ErrorPage",
                            "default": "/ErrorPage.htm",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "URL fragment of error page",
                            "cli-alias": "error-url",
                            "display": "Error",
                            "description": "The URL fragment of the error page on the application server. The error page is displayed after a login failure. The default is /ErrorPage.htm."
                        },
                        {
                            "name": "LogoutPage",
                            "default": "/LogoutPage.htm",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "URL of logout page",
                            "cli-alias": "logout-url",
                            "display": "Logout",
                            "description": "The URL fragment of the logout page on the application server. An empty string disables logout. The default is /LogoutPage.htm."
                        },
                        {
                            "name": "DefaultURL",
                            "default": "/",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "URL fragment to go to after successful login",
                            "cli-alias": "default-url",
                            "display": "Default URL",
                            "description": "The URL fragment to visit after a successful login when no other target is available. In most cases, login is triggered via an attempt to access a URL in the application, and the client is redirected to this URL after successfully logging in. However, if users explicitly visit the URL of the login form, they are redirected to this URL after logging in. This URL fragment is relative to the root directory. Although the default is /, you must specify the filename of the page to go after a successful login."
                        },
                        {
                            "name": "FormsLocation",
                            "default": "backend",
                            "required-when": {
                                "condition": {
                                    "property-name": "FormSupportType",
                                    "evaluation": "property-equals",
                                    "value": "static"
                                }
                            },
                            "summary": "Location of HTML pages",
                            "cli-alias": "forms-location",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmFormsLocation"
                            },
                            "display": "Forms Storage Location",
                            "description": "The location for where the DataPower Gateway retrieves the login, logout and error pages. The default is the remote application server."
                        },
                        {
                            "name": "LocalLoginForm",
                            "default": "store:///LoginPage.htm",
                            "required-when": {
                                "condition": {
                                    "evaluation": "logical-and",
                                    "condition": [
                                        {
                                            "property-name": "FormsLocation",
                                            "evaluation": "property-equals",
                                            "value": "appliance"
                                        },
                                        {
                                            "property-name": "FormSupportType",
                                            "evaluation": "property-equals",
                                            "value": "static"
                                        }
                                    ]
                                }
                            },
                            "locations": {
                                "location": [
                                    "local",
                                    "store"
                                ]
                            },
                            "summary": "Location of local login form",
                            "cli-alias": "local-login-page",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-or",
                                    "condition": [
                                        {
                                            "property-name": "FormSupportType",
                                            "evaluation": "property-does-not-equal",
                                            "value": "static"
                                        },
                                        {
                                            "property-name": "FormsLocation",
                                            "evaluation": "property-equals",
                                            "value": "backend"
                                        }
                                    ]
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmFSFile"
                            },
                            "display": "On-DataPower Gateway Login Form",
                            "description": "The location of the login form on the DataPower Gateway. The default is store:///LoginPage.htm."
                        },
                        {
                            "name": "LocalErrorPage",
                            "default": "store:///ErrorPage.htm",
                            "required-when": {
                                "condition": {
                                    "evaluation": "logical-and",
                                    "condition": [
                                        {
                                            "property-name": "FormsLocation",
                                            "evaluation": "property-equals",
                                            "value": "appliance"
                                        },
                                        {
                                            "property-name": "FormSupportType",
                                            "evaluation": "property-equals",
                                            "value": "static"
                                        }
                                    ]
                                }
                            },
                            "locations": {
                                "location": [
                                    "local",
                                    "store"
                                ]
                            },
                            "summary": "Location of local error page",
                            "cli-alias": "local-error-page",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-or",
                                    "condition": [
                                        {
                                            "property-name": "FormSupportType",
                                            "evaluation": "property-does-not-equal",
                                            "value": "static"
                                        },
                                        {
                                            "property-name": "FormsLocation",
                                            "evaluation": "property-equals",
                                            "value": "backend"
                                        }
                                    ]
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmFSFile"
                            },
                            "display": "On-DataPower Gateway Error Page",
                            "description": "The location of the error page on the DataPower Gateway. The default is store:///ErrorPage.htm."
                        },
                        {
                            "name": "LocalLogoutPage",
                            "default": "store:///LogoutPage.htm",
                            "required-when": {
                                "condition": {
                                    "evaluation": "logical-and",
                                    "condition": [
                                        {
                                            "property-name": "FormsLocation",
                                            "evaluation": "property-equals",
                                            "value": "appliance"
                                        },
                                        {
                                            "property-name": "FormSupportType",
                                            "evaluation": "property-equals",
                                            "value": "static"
                                        }
                                    ]
                                }
                            },
                            "locations": {
                                "location": [
                                    "local",
                                    "store"
                                ]
                            },
                            "summary": "Location of local logout page",
                            "cli-alias": "local-logout-page",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-or",
                                    "condition": [
                                        {
                                            "property-name": "FormSupportType",
                                            "evaluation": "property-does-not-equal",
                                            "value": "static"
                                        },
                                        {
                                            "property-name": "FormsLocation",
                                            "evaluation": "property-equals",
                                            "value": "backend"
                                        }
                                    ]
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmFSFile"
                            },
                            "display": "On-DataPower Gateway Logout Page",
                            "description": "The location of the logout page on the DataPower Gateway. The default is store:///LogoutPage.htm."
                        },
                        {
                            "name": "UsernameField",
                            "default": "j_username",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "Attribute value of the user name field",
                            "cli-alias": "username-field",
                            "display": "User Name Field Name",
                            "description": "The value of the 'name' attribute for the user name field on the login form. The default is j_username."
                        },
                        {
                            "name": "PasswordField",
                            "default": "j_password",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "Attribute value of the password field",
                            "cli-alias": "password-field",
                            "display": "Password Field Name",
                            "description": "The value of the 'name' attribute for the password field on the login form. The default is j_password."
                        },
                        {
                            "name": "RedirectField",
                            "default": "originalUrl",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "Attribute value of the hidden field for the target URL",
                            "cli-alias": "redirect-field",
                            "display": "Target URL Field Name",
                            "description": "The value of the 'name' attribute for the hidden field on the login form. This field stores the original target URL, which is the URL that the client is attempting to access. The default is originalUrl."
                        },
                        {
                            "name": "FormProcessingURL",
                            "default": "/j_security_check",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "Processing URL fragment",
                            "cli-alias": "post-action-url",
                            "display": "Form POST Action URL",
                            "description": "The URL fragment to which the login form posts its results when a user submits the request. The default is /j_security_check."
                        },
                        {
                            "name": "InactivityTimeout",
                            "default": 600,
                            "required-when": {
                                "condition": {
                                    "property-name": "UseCookieAttributes",
                                    "evaluation": "property-equals",
                                    "value": "no"
                                }
                            },
                            "summary": "Allowable duration for an inactive session",
                            "cli-alias": "inactivity-timeout",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "units": "Seconds",
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Inactivity Timeout",
                            "description": "The number of seconds before an inactive session is invalidated. The default is 600. A value of 0 disables the inactivity timer."
                        },
                        {
                            "name": "SessionLifetime",
                            "default": 10800,
                            "required-when": {
                                "condition": {
                                    "property-name": "UseCookieAttributes",
                                    "evaluation": "property-equals",
                                    "value": "no"
                                }
                            },
                            "summary": "Maximum time before requiring re-authentication",
                            "cli-alias": "session-lifetime",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
