                "uri": "config/api-definition",
                "summary": "Configure and manage API Definitions",
                "cmd-group": "configuration",
                "cli-alias": "api-definition",
                "properties": {
                    "property": [
                        {
                            "name": "mAdminState",
                            "default": "enabled",
                            "type": {
                                "href": "/mgmt/types/default/dmAdminState"
                            },
                            "label": "Enable administrative state",
                            "summary": "Set the administrative state of this configuration.",
                            "cli-alias": "admin-state",
                            "display": "Administrative state",
                            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>",
                            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
                        },
                        {
                            "description": "A descriptive summary for the configuration.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Enter a brief descriptive summary for the configuration.",
                            "cli-alias": "summary",
                            "display": "Comments",
                            "name": "UserSummary"
                        },
                        {
                            "description": "Specifies the ID of the API.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "ID of the API",
                            "cli-alias": "id",
                            "display": "API ID",
                            "name": "ID"
                        },
                        {
                            "description": "Specifies the name of the API.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Name of the API",
                            "cli-alias": "name",
                            "display": "API name",
                            "name": "Name"
                        },
                        {
                            "name": "Version",
                            "default": "1.0.0",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Version of the API",
                            "cli-alias": "version",
                            "display": "API version",
                            "description": "Specifies the version of the API."
                        },
                        {
                            "name": "BasePath",
                            "default": "/",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "The base path for the API",
                            "cli-alias": "base-path",
                            "valid-regexp": "^\\/$||^\\/([0-9a-zA-Z-_.~%!$&amp;'()*+,;=:@]+\\/)*[0-9a-zA-Z-_.~%!$&amp;'()*+,;=:@]+$",
                            "display": "Base path",
                            "description": "Specifies the base path on which the API is served, which is relative to the host. When the base path is not specified, the APIs are served directly under the host. The base path does not include the host name or any additional segments for paths or operations. The base path must start but not end with slash (/). All resources in a REST API are defined relative to its base path."
                        },
                        {
                            "name": "HtmlPage",
                            "type": {
                                "href": "/mgmt/types/default/dmFSFile"
                            },
                            "locations": {
                                "location": [
                                    "local",
                                    "temporary",
                                    "store"
                                ]
                            },
                            "summary": "Location of a static HTML page",
                            "cli-alias": "html-page",
                            "display": "HTML page",
                            "description": "Specifies the name and location of a static HTML page that the API can return. Import the file to the &lt;tt>local:&lt;/tt> directory, the &lt;tt>store:&lt;/tt> directory, or the &lt;tt>temporary:&lt;/tt> directory, or one their subdirectories in the DataPower file system."
                        },
                        {
                            "name": "Type",
                            "default": "standard",
                            "type": {
                                "href": "/mgmt/types/default/dmAPIType"
                            },
                            "summary": "Type of the API",
                            "cli-alias": "type",
                            "display": "Type",
                            "description": "Specifies the API type."
                        },
                        {
                            "description": "Specifies the assembly to apply to API calls. An assembly comprises a rule that defines the actions to run against the API call and how to handle the errors during assembly execution.",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/Assembly"
                                }
                            },
                            "summary": "The assembly for API invocation",
                            "cli-alias": "assembly",
                            "display": "Assembly",
                            "name": "Assembly"
                        },
                        {
                            "name": "Path",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/APIPath"
                                }
                            },
                            "required": "true",
                            "summary": "Routes to access REST APIs",
                            "array": "true",
                            "cli-alias": "path",
                            "display": "Paths",
                            "description": "Specifies a list of paths through which users access the API operations. A path consists of one or more HTTP operations."
                        },
                        {
                            "name": "Consume",
                            "valid-regexp": "[a-zA-Z0-9][a-zA-Z0-9!#$&amp;\\-^_]{0,126}\\/[a-zA-Z0-9][a-zA-Z0-9!#$&amp;\\-^_+.]{0,126}(;\\s*[a-zA-Z0-9][a-zA-Z0-9!#$&amp;\\-^_=\"]{0,126})*$",
                            "summary": "MIME types that the API can consume",
                            "cli-alias": "consume",
                            "array": "true",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "display": "Consumes",
                            "description": "Specifies a list of MIME types that the API can consume. These MIME types apply to all API operations. You can override the setting for specific operations in the API operation."
                        },
                        {
                            "name": "Produce",
                            "valid-regexp": "^(application|audio|image|message|multipart|text|video)\\/[a-zA-Z-]+$",
                            "summary": "MIME types that the API can produce",
                            "cli-alias": "produce",
                            "array": "true",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "display": "Produces",
                            "description": "Specifies a list of MIME types that the API can produce. These MIME types apply to all API operations. You can override the setting for specific operations in the API operation."
                        },
                        {
                            "name": "SwaggerLocation",
                            "type": {
                                "href": "/mgmt/types/default/dmFSFile"
                            },
                            "locations": {
                                "location": [
                                    "local",
                                    "temporary"
                                ]
                            },
                            "summary": "The name and location of the Swagger document",
                            "cli-alias": "swagger-location",
                            "display": "Swagger location",
                            "description": "&lt;p>When you create the API definition from a Swagger document, specify the name and location of the Swagger document. Prepare the document as follows before you specify the location.&lt;/p>&lt;ol>&lt;li>When the Swagger document is a YAML file, convert it to JSON format.&lt;/li>&lt;li>Import the JSON file to the &lt;tt>local:&lt;/tt> directory, the &lt;tt>temporary:&lt;/tt> directory, or one their subdirectories in the DataPower file system.&lt;/li>&lt;/ol>&lt;p>When you create the API definition by manually specifying the API properties, this field is not applicable.&lt;/p>"
                        },
                        {
                            "name": "GraphQLSchema",
                            "required-when": {
                                "condition": {
                                    "property-name": "Type",
                                    "evaluation": "property-equals",
                                    "value": "graphql"
                                }
                            },
                            "summary": "GraphQL schema location.",
                            "cli-alias": "graphql-schema",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "Type",
                                    "evaluation": "property-does-not-equal",
                                    "value": "graphql"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/APISchema"
                                }
                            },
                            "display": "GraphQL schema location",
                            "description": "GraphQL schema location."
                        },
                        {
                            "name": "WsdlAdvertisedSchemaLocation",
                            "type": {
                                "href": "/mgmt/types/default/dmFSFile"
                            },
                            "summary": "The name and location of the WSDL file or archive file with a WSDL file, which should be used to advertise the contract for this API.",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "Type",
                                    "evaluation": "property-does-not-equal",
                                    "value": "wsdl"
                                }
                            },
                            "cli-alias": "wsdl-advertised-schema-location",
                            "display": "WSDL advertised schema location",
                            "description": "When you create the API definition from a WSDL file, specify the name and location of the WSDL document or archive of the WSDL and its dependencies."
                        },
                        {
                            "name": "WsdlValidationSchema",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/APISchema"
                                }
                            },
                            "summary": "The name and location of the top-level WSDL file to use to validate SOAP payloads.",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "Type",
                                    "evaluation": "property-does-not-equal",
                                    "value": "wsdl"
                                }
                            },
                            "cli-alias": "wsdl-validation-schema",
                            "display": "WSDL validation schema location",
                            "description": "When you create the API definition from a WSDL file, specify the name and location of the top-level WSDL document itself, which should be used to validate incoming SOAP payloads."
                        },
                        {
                            "name": "SecurityRequirement",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/APISecurityRequirement"
                                }
                            },
                            "summary": "Required security schemes to execute API operations",
                            "array": "true",
                            "cli-alias": "security-req",
                            "display": "Security requirements",
                            "description": "Specifies the alternative security requirements to enforce for the API as a whole (that is, there is a logical OR between the security requirements). By default, the security requirement is applied to all operations in the API. But for each API operation, you can override the API-level security by separately specifying security schemes to enforce on the operation level."
                        },
                        {
                            "name": "RequireAPIMutualTLS",
                            "default": "off",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Require mutual TLS protection",
                            "cli-alias": "require-api-mutual-tls",
                            "display": "API protection",
                            "description": "Require mutual TLS protection for this API."
                        },
                        {
                            "name": "APIMutualTLSSource",
                            "required-when": {
                                "condition": {
                                    "property-name": "RequireAPIMutualTLS",
                                    "evaluation": "property-equals",
                                    "value": "on"
                                }
                            },
                            "summary": "Sources to obtain the client certificate for mutual TLS",
                            "cli-alias": "api-mutual-tls-source",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "RequireAPIMutualTLS",
                                    "evaluation": "property-does-not-equal",
                                    "value": "on"
                                }
                            },
                            "array": "true",
                            "type": {
                                "href": "/mgmt/types/default/dmAPIMutualTLSSource"
                            },
                            "display": "API protection sources",
                            "description": "Indicates the sources to obtain the client certificate for mutual TLS. Because you can define multiple ways to obtain the source, ensure that you sequence the methods appropriately."
                        },
                        {
                            "name": "APIMutualTLSHeaderName",
                            "default": "X-Client-Certificate",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
