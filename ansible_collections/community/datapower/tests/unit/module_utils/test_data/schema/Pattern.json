                            "summary": "Set the administrative state of this configuration.",
                            "cli-alias": "admin-state",
                            "display": "Administrative state",
                            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>",
                            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
                        },
                        {
                            "name": "UserSummary",
                            "valid-regexp": "^.{0,128}$",
                            "summary": "Pattern name",
                            "cli-alias": "name",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "display": "Pattern name",
                            "hoverhelp": "Specify the name of the pattern that is displayed in the user interface.",
                            "description": "Brief summary of the Pattern."
                        },
                        {
                            "description": "Specifies the user annotation of the pattern.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Brief summary for user annotation",
                            "cli-alias": "description",
                            "display": "Documentation",
                            "hoverhelp": "The documentation for the pattern in html or plain text, which appears on the Pattern browser.",
                            "name": "Description"
                        },
                        {
                            "description": "Indicates when the pattern is created.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Time of creation",
                            "cli-alias": "createDate",
                            "display": "Created on",
                            "name": "CreateDate"
                        },
                        {
                            "description": "Indicates when the pattern is last modified.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Time of last modification",
                            "cli-alias": "modifyDate",
                            "display": "Last Modified on",
                            "name": "ModifyDate"
                        },
                        {
                            "description": "Indicates the user ID of the pattern author.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "User id of the pattern author",
                            "cli-alias": "author",
                            "display": "Author",
                            "name": "Author"
                        },
                        {
                            "description": "Indicates which person or entity owns this pattern.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Owner of the pattern",
                            "cli-alias": "owner",
                            "display": "Owner",
                            "hoverhelp": "Person or entity that owns the pattern.",
                            "name": "Owner"
                        },
                        {
                            "description": "Specifies which category the pattern belongs to.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Category of the pattern",
                            "cli-alias": "category",
                            "display": "Category",
                            "hoverhelp": "Specify which category the pattern belongs to. Select an existing category or enter a new category.",
                            "name": "Category"
                        },
                        {
                            "description": "A version is a string that can be any combination of any characters.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Version of the pattern",
                            "cli-alias": "version",
                            "display": "Version",
                            "hoverhelp": "Specify the version of the pattern. A version is a string that can be any combination of any characters.",
                            "name": "Version"
                        },
                        {
                            "name": "Tags",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Keywords of the pattern",
                            "array": "true",
                            "cli-alias": "tags",
                            "display": "Keywords",
                            "hoverhelp": "Specify the keywords to associate with the pattern. A pattern can have multiple keywords. To add a keyword, press Enter or Space after you enter the keyword.",
                            "description": "Specifies the keywords that are associated with the pattern. A pattern can have multiple keywords."
                        },
