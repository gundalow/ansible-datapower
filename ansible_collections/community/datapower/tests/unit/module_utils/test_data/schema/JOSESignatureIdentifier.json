        "metadata": {
            "object": {
                "licensed-feature": "IDG",
                "name": "JOSESignatureIdentifier",
                "display": "Signature Identifier",
                "property-group": {
                    "member": [
                        {
                            "name": "mAdminState"
                        },
                        {
                            "name": "UserSummary"
                        },
                        {
                            "name": "Type"
                        },
                        {
                            "name": "SSKey"
                        },
                        {
                            "name": "Certificate"
                        },
                        {
                            "name": "ValidAlgorithms"
                        },
                        {
                            "name": "HeaderParam"
                        },
                        {
                            "name": "Verify"
                        }
                    ],
                    "name": "main",
                    "display": "Main"
                },
                "uri": "crypto/jose-signature-identifier",
                "summary": "JOSE Signature Identifier object for the JSON Web Verify",
                "cmd-group": "crypto",
                "cli-alias": "jose-signature-identifier",
                "properties": {
                    "property": [
                        {
                            "name": "mAdminState",
                            "default": "enabled",
                            "type": {
                                "href": "/mgmt/types/default/dmAdminState"
                            },
                            "label": "Enable administrative state",
                            "summary": "Set the administrative state of this configuration.",
                            "cli-alias": "admin-state",
                            "display": "Administrative state",
                            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>",
                            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
                        },
                        {
                            "description": "A descriptive summary for the configuration.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Enter a descriptive summary for the configuration.",
                            "cli-alias": "summary",
                            "display": "Comments",
                            "name": "UserSummary"
                        },
                        {
                            "description": "Key material type used to verify the signature.",
                            "type": {
                                "href": "/mgmt/types/default/dmVerifyKeyType"
                            },
                            "summary": "Key material type used to verify the signature",
                            "cli-alias": "type",
                            "display": "Key Material Type",
                            "name": "Type"
                        },
                        {
                            "name": "SSKey",
                            "required-when": {
                                "condition": {
                                    "property-name": "Type",
                                    "evaluation": "property-value-in-list",
                                    "value": "sskey"
                                }
                            },
                            "summary": "Shared secret key",
                            "cli-alias": "sskey",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/CryptoSSKey"
                                }
                            },
                            "display": "Key Material",
                            "description": "Use the shared secret key to verify the signature."
                        },
                        {
                            "name": "Certificate",
                            "required-when": {
                                "condition": {
                                    "property-name": "Type",
                                    "evaluation": "property-value-in-list",
                                    "value": "certificate"
                                }
                            },
                            "summary": "Certificate",
                            "cli-alias": "cert",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/CryptoCertificate"
                                }
                            },
                            "display": "Key Material",
                            "description": "Use the certificate to verify the signature."
                        },
                        {
                            "name": "ValidAlgorithms",
                            "type": {
                                "href": "/mgmt/types/default/dmJWSSignAlgorithm"
                            },
                            "summary": "Valid alg header parameter values for the JWS verify action",
                            "array": "true",
                            "cli-alias": "alg",
                            "display": "Valid algorithms",
                            "description": "Specifies an array of algorithm values that are valid for signature verification. When specified, the JWS &lt;tt>alg&lt;/tt> header parameter value must match a value in this set. By default, all allowed JWS &lt;tt>alg&lt;/tt> header parameters values are valid."
                        },
                        {
                            "name": "HeaderParam",
                            "type": {
                                "href": "/mgmt/types/default/dmJOSEHeader"
                            },
                            "required": "true",
                            "summary": "The JOSE header parameters used to identify the signature",
                            "array": "true",
                            "cli-alias": "header-param",
                            "display": "Header Parameters",
                            "description": "The JOSE header parameters used to identify the signature."
                        },
                        {
                            "name": "Verify",
                            "default": "on",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "required": "true",
                            "summary": "Deprecated - Whether to perform verification on the identified signature.",
                            "cli-alias": "verify",
                            "display": "Verify (deprecated)",
                            "description": "Deprecated - The verify toggle indicates whether to perform verification on the identified signature."
                        }
                    ]
                },
                "description": "JOSE Signature Identifier object for the JSON Web Verify."
            },
            "_links": {
                "doc": {
                    "href": "/mgmt/docs/metadata/JOSESignatureIdentifier"
                },
                "self": {
                    "href": "/mgmt/metadata/default/JOSESignatureIdentifier"
                }
            }
        }
    }
}