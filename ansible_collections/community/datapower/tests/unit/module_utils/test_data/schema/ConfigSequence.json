{
    "schema": {
        "types": [
            {
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmAdminState"
                    },
                    "self": {
                        "href": "/mgmt/types/default/dmAdminState"
                    }
                },
                "type": {
                    "cli-arg": "enabled | disabled",
                    "name": "dmAdminState",
                    "value-list": {
                        "value": [
                            {
                                "name": "enabled",
                                "display": "enabled"
                            },
                            {
                                "name": "disabled",
                                "display": "disabled"
                            }
                        ]
                    }
                }
            },
            {
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmString"
                    },
                    "self": {
                        "href": "/mgmt/types/default/dmString"
                    }
                },
                "type": {
                    "cli-arg": "string",
                    "name": "dmString"
                }
            },
            {
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmConfigSequenceLocation"
                    },
                    "self": {
                        "href": "/mgmt/types/default/dmConfigSequenceLocation"
                    }
                },
                "type": {
                    "properties": {
                        "property": [
                            {
                                "name": "Directory",
                                "default": "local:///",
                                "type": {
                                    "href": "/mgmt/types/default/dmFSDirectory"
                                },
                                "locations": {
                                    "location": [
                                        "config",
                                        "local",
                                        "temporary"
                                    ]
                                },
                                "summary": "Directory in the file system to watch for configuration file changes",
                                "cli-alias": "directory",
                                "display": "Location",
                                "description": "Specifies the directory where configuration files are stored. DataPower scans the directory for configuration files that match a given PCRE pattern and runs only the matching files. By default, this directory is also where DataPower stores the output files generated by running the configuration files."
                            },
                            {
                                "description": "Specifies a list of access policies to enforce when running the files in the location, each policy defines permissions to a specific group of resources.",
                                "type": {
                                    "href": "/mgmt/types/default/dmReference",
                                    "reference-to": {
                                        "href": "/mgmt/metadata/default/AccessProfile"
                                    }
                                },
                                "summary": "Access profile that defines permissions to resources in the location",
                                "cli-alias": "access-profile",
                                "display": "Access profile",
                                "name": "AccessProfileName"
                            }
                        ]
                    },
                    "display": "Location profile",
                    "name": "dmConfigSequenceLocation"
                }
            },
            {
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmPCRE"
                    },
                    "self": {
                        "href": "/mgmt/types/default/dmPCRE"
                    }
                },
                "type": {
                    "cli-arg": "pcre",
                    "name": "dmPCRE"
                }
            },
            {
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmToggle"
                    },
                    "self": {
                        "href": "/mgmt/types/default/dmToggle"
                    }
                },
                "type": {
                    "cli-arg": "on | off",
                    "name": "dmToggle",
                    "value-list": {
                        "value": [
                            {
                                "name": "on",
                                "display": "on"
                            },
                            {
                                "name": "off",
                                "display": "off"
                            }
                        ]
                    }
                }
            },
            {
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmFSDirectory"
                    },
                    "self": {
                        "href": "/mgmt/types/default/dmFSDirectory"
                    }
                },
                "type": {
                    "cli-arg": "directory",
                    "name": "dmFSDirectory"
                }
            },
            {
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmUInt32"
                    },
                    "self": {
                        "href": "/mgmt/types/default/dmUInt32"
                    }
                },
                "type": {
                    "cli-arg": "number",
                    "minimum": 0,
                    "name": "dmUInt32",
                    "maximum": "0xFFFFFFFF"
                }
            },
            {
                "_links": {
                    "doc": {
                        "href": "/mgmt/docs/types/dmReference"
                    },
                    "self": {
                        "href": "/mgmt/types/default/dmReference"
                    }
                },
                "type": {
                    "cli-arg": "object",
                    "name": "dmReference"
                }
            }
        ],
        "metadata": {
            "object": {
                "licensed-feature": "IDG",
                "name": "ConfigSequence",
                "display": "Configuration Sequence",
                "property-group": {
                    "member": [
                        {
                            "name": "mAdminState"
                        },
                        {
                            "name": "UserSummary"
                        },
                        {
                            "name": "Locations"
                        },
                        {
                            "name": "MatchPattern"
                        },
                        {
                            "name": "ResultNamePattern"
                        },
                        {
                            "name": "StatusNamePattern"
                        },
                        {
                            "name": "Watch"
                        },
                        {
                            "name": "UseOutputLocation"
                        },
                        {
                            "name": "OutputLocation"
                        },
                        {
                            "name": "DeleteUnused"
                        },
                        {
                            "name": "RunSequenceInterval"
                        }
                    ],
                    "name": "main"
                },
                "uri": "config/sequence",
                "summary": "Configuration Sequence",
                "cmd-group": "configuration",
                "cli-alias": "config-sequence",
                "properties": {
                    "property": [
                        {
                            "name": "mAdminState",
                            "default": "enabled",
                            "type": {
                                "href": "/mgmt/types/default/dmAdminState"
                            },
                            "label": "Enable administrative state",
                            "summary": "Set the administrative state of this configuration.",
                            "cli-alias": "admin-state",
                            "display": "Administrative state",
                            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>",
                            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
                        },
                        {
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "display": "Comments",
                            "cli-alias": "summary",
                            "name": "UserSummary",
                            "summary": "Brief summary for user annotation."
                        },
                        {
                            "name": "Locations",
                            "type": {
                                "href": "/mgmt/types/default/dmConfigSequenceLocation"
                            },
                            "summary": "Ordered list of locations to watch for changes and load configuration, and associated permissions for each location",
                            "array": "true",
                            "cli-alias": "location",
                            "display": "Location profiles",
                            "description": "Each entry specifies a directory in the file system where the configuration files to match are stored. The DataPower Gateway watches the specified location and reloads the configuration when a change is detected in configuration files that match the specified PCRE match pattern. The entries are processed in the order that they appear in the list. The assess profile, when specified, indicates the permissions for the file execution."
                        },
                        {
                            "name": "MatchPattern",
                            "default": "(.*)\\.cfg$",
                            "type": {
                                "href": "/mgmt/types/default/dmPCRE"
                            },
                            "summary": "PCRE pattern to match configuration files for the match location",
                            "cli-alias": "match-pattern",
                            "display": "Matching pattern",
                            "description": "Specifies the PCRE match pattern that determines whether a file should be considered part of the match location. For instance, when the configuration files to match are &lt;tt>NNNNNN.input&lt;/tt> , then the PCRE pattern is &lt;tt>\"([0-9]{6})\\.input$\"&lt;/tt> ."
                        },
                        {
                            "name": "ResultNamePattern",
                            "default": "$1.log",
                            "type": {
                                "href": "/mgmt/types/default/dmPCRE"
                            },
                            "summary": "PCRE pattern to name the generated result files",
                            "cli-alias": "result-name-pattern",
                            "display": "Result file naming pattern",
                            "description": "&lt;p>Enter the PCRE pattern to use to build the name of the result file. This PCRE pattern normally has a back-reference to the base input file name. For instance, when input files are &lt;tt>NNNNNN.input&lt;/tt> and the desired result file name is &lt;tt>NNNNNN.result&lt;/tt> , then the pattern is &lt;tt>\"$1.result\"&lt;/tt> .&lt;/p>"
                        },
                        {
                            "name": "StatusNamePattern",
                            "default": "$1.status",
                            "type": {
                                "href": "/mgmt/types/default/dmPCRE"
                            },
                            "summary": "PCRE pattern to name the generated status files",
                            "cli-alias": "status-name-pattern",
                            "display": "Status file naming pattern",
                            "description": "&lt;p>Enter the PCRE to use to build the name of the JSON-format object status file. This PCRE pattern normally has a back-reference for the base input file name. For instance, when the input files are &lt;tt>NNNNNN.input&lt;/tt> and the desired object status file name is &lt;tt>NNNNNN.json&lt;/tt> , then the pattern is &lt;tt>\"$1.json\"&lt;/tt> .&lt;/p>"
                        },
                        {
                            "name": "Watch",
                            "default": "on",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Whether to watch changes in the matching configuration files",
                            "cli-alias": "watch",
                            "display": "Watch",
                            "description": "Specifies whether to watch the specified directory for configuration file changes and automatically reload the configuration when a change is detected. By default, the specified directory is watched."
                        },
                        {
                            "name": "UseOutputLocation",
                            "default": "off",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Whether to place output log and status files in the configured output location",
                            "cli-alias": "use-output-location",
                            "display": "Use output location",
                            "description": "Specifies whether to place output log and status files in the configured output location after the files are processed. By default, output files are stored in the input file location."
                        },
                        {
                            "name": "OutputLocation",
                            "default": "logtemp:///",
                            "required-when": {
                                "condition": {
                                    "property-name": "UseOutputLocation",
                                    "evaluation": "property-equals",
                                    "value": "on"
                                }
                            },
                            "locations": {
                                "location": [
                                    "logtemp",
                                    "local",
                                    "temporary"
                                ]
                            },
                            "summary": "Directory to store the output log files and status files",
                            "cli-alias": "output-location",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "UseOutputLocation",
                                    "evaluation": "property-equals",
                                    "value": "off"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmFSDirectory"
                            },
                            "display": "Output file location",
                            "description": "Specifies the directory to store the output files that are generated for the execution of the loaded configuration files. When not specified, the input file location is used."
                        },
                        {
                            "name": "DeleteUnused",
                            "default": "on",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Whether to clean up objects no longer needed after configuration events",
                            "cli-alias": "delete-unused",
                            "display": "Delete unused",
                            "description": "Specifies whether to enable cleaning up objects that are no longer needed. When enabled, the configuration sequence detects and attempts to delete the objects that are no longer modified by any configuration file. By default, the configuration sequence does not delete unneeded objects."
                        },
                        {
                            "name": "RunSequenceInterval",
                            "default": 100,
                            "cli-alias": "run-sequence-interval",
                            "maximum": 60000,
                            "summary": "Time delay between a file event occurrence and a new sequence running",
                            "minimum": 100,
                            "units": "milliseconds",
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Configuration execution interval",
                            "description": "Specifies how long, after a file event occurs, to wait before running this sequence again to load file changes. This delay enables multiple file events to be aggregated and handled within the same sequence run. The unit of the value is milliseconds. Enter a value in the range 100 - 60000. By default, a sequence run will load changes caused by file events that occur during the last 100 milliseconds."
                        }
                    ]
                },
                "description": "A sequence of configuration files to load after the start-up configuration. By default, changes on the configuration files are automatically detected and reloaded."
            },
            "_links": {
                "doc": {
                    "href": "/mgmt/docs/metadata/ConfigSequence"
                },
                "self": {
                    "href": "/mgmt/metadata/default/ConfigSequence"
                }
            }
        }
    }
}