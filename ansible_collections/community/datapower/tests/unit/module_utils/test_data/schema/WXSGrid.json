        "metadata": {
            "object": {
                "licensed-feature": "IDG",
                "name": "WXSGrid",
                "display": "eXtreme Scale Grid",
                "property-group": {
                    "member": [
                        {
                            "name": "Collective"
                        },
                        {
                            "name": "Grid"
                        }
                    ],
                    "name": "summary",
                    "display": "Summary"
                },
                "uri": "network/wxs-grid",
                "summary": "Connection details to an eXtreme Scale grid in an eXtreme Scale collective",
                "cmd-group": "configuration",
                "cli-alias": "wxs-grid",
                "properties": {
                    "property": [
                        {
                            "name": "mAdminState",
                            "default": "enabled",
                            "type": {
                                "href": "/mgmt/types/default/dmAdminState"
                            },
                            "label": "Enable administrative state",
                            "summary": "Set the administrative state of this configuration.",
                            "cli-alias": "admin-state",
                            "display": "Administrative state",
                            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>",
                            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
                        },
                        {
                            "description": "Specify a brief, but descriptive, summary of the configuration.",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "summary": "Descriptive summary of the configuration",
                            "cli-alias": "summary",
                            "display": "Comments",
                            "name": "UserSummary"
                        },
                        {
                            "name": "Collective",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/LoadBalancerGroup"
                                }
                            },
                            "required": "true",
                            "summary": "Load Balancer Group that corresponds to a collective of eXtreme Scale",
                            "cli-alias": "collective",
                            "display": "Collective",
                            "description": "Specify the Load Balancer Group that contains members in the collective of eXtreme Scale. You must define at least one member in the collective."
                        },
                        {
                            "name": "Grid",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "Name of the eXtreme Scale grid",
                            "cli-alias": "grid",
                            "valid-regexp": "^[^\\\\\\^\\]./,#$@:;\"'*?&lt;>|=+&amp;%[\\s]+$",
                            "display": "Grid Name",
                            "description": "Specify the name of the eXtreme Scale grid. The value cannot contain whitespace or the following characters: &lt;tt>^ . \\ / , # $ @ : ; * ? &amp;lt; > | = + &amp;amp; % [ ] \" '&lt;/tt> ."
                        },
                        {
                            "name": "UserName",
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "required": "true",
                            "summary": "User who connects to the eXtreme Scale collective",
                            "cli-alias": "username",
                            "valid-regexp": "^[^&lt;#\\s]{1,64}$",
                            "display": "User",
                            "description": "&lt;p>Specify the user account of the eXtreme Scale user who connects to the eXtreme Scale collective. The value can be up to 64 characters in length and cannot be blank. You can use all alphanumeric characters and most special characters. You cannot use spaces or the following special characters: &lt;tt># &amp;lt;&lt;/tt> .&lt;/p>&lt;p>The user must have sufficient eXtreme Scale permissions to access the grid.&lt;/p>"
                        },
                        {
                            "name": "PasswordAlias",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/PasswordAlias"
                                }
                            },
                            "required": "true",
                            "summary": "Password alias of the password for the user who connects to the eXtreme Scale collective",
                            "cli-alias": "password-alias",
                            "display": "Password Alias",
                            "description": "Specify the password alias to use to look up the password of the eXtreme Scale user who connects to the eXtreme Scale collective."
                        },
                        {
                            "name": "Timeout",
                            "default": 1000,
                            "cli-alias": "timeout",
                            "required": "true",
                            "maximum": 86400000,
                            "summary": "Time limit to establish a connection",
                            "minimum": 10,
                            "units": "ms",
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Timeout",
                            "description": "Specify the maximum time to wait to establish a connection to an eXtreme Scale. If unable to establish a connection, the operation fails. Enter a value in the range 10 - 86400000. The default value is 1000."
                        },
                        {
                            "description": "The TLS client profile to secure connections between the DataPower Gateway and its targets.",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/SSLClientProfile"
                                }
                            },
                            "summary": "Set the TLS client profile to secure connections between the DataPower Gateway and its targets",
                            "cli-alias": "ssl-client",
                            "display": "TLS client profile",
                            "name": "SSLClient"
                        },
                        {
                            "name": "Encrypt",
                            "default": "off",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Whether the data in the eXtreme Scale data grid is encrypted",
                            "cli-alias": "encrypt",
                            "display": "Encrypted Data",
                            "description": "Indicates whether the data in the eXtreme Scale data grid is encrypted. If encrypted, the data is encrypted, when writing to, and decrypted, when reading from, the eXtreme Scale data grid."
                        },
                        {
                            "name": "EncryptSSKey",
                            "required-when": {
                                "condition": {
                                    "property-name": "Encrypt",
                                    "evaluation": "property-equals",
                                    "value": "on"
                                }
                            },
                            "summary": "Shared secret key for PKCS #7 encryption and decryption",
                            "cli-alias": "encrypt-sskey",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/CryptoSSKey"
                                }
                            },
                            "display": "Shared Secret Key for Encryption and Decryption",
                            "description": "Specify the shared secret for PKCS #7 encryption and decryption. When writing data to the data grid, encrypts the data. When reading data from the eXtreme Scale data grid, decrypts the data."
                        },
                        {
                            "name": "EncryptAlg",
                            "default": "tripledes-cbc",
                            "required-when": {
                                "condition": {
                                    "property-name": "Encrypt",
                                    "evaluation": "property-equals",
                                    "value": "on"
                                }
                            },
                            "summary": "Algorithm that encrypt and decrypt data transfers with the eXtreme Scale collective",
                            "cli-alias": "encrypt-alg",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmPKCS7CryptoEncryptionAlgorithm"
                            },
                            "display": "PKCS #7 algorithm for encryption and decryption",
                            "description": "Specify the PKCS #7 algorithm for encryption and decryption. When writing data to the data grid, encrypts the data. When reading data from the eXtreme Scale data grid, decrypts the data."
                        },
                        {
                            "name": "KeyObfuscation",
                            "default": "off",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Whether to apply a hash algorithm for key-obfuscation",
                            "cli-alias": "key-obfuscation",
                            "display": "Hash Key Obfuscation",
                            "description": "Indicate whether to apply a hash algorithm to obfuscate keys before reading data from or writing data to the eXtreme Scale data grid."
                        },
                        {
                            "name": "KeyObfuscationAlg",
                            "default": "sha256",
                            "required-when": {
                                "condition": {
                                    "property-name": "KeyObfuscation",
                                    "evaluation": "property-equals",
                                    "value": "on"
                                }
                            },
                            "summary": "Hash algorithm for key-obfuscation",
                            "cli-alias": "key-obfuscation-alg",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmCryptoHashAlgorithm"
                            },
                            "display": "Hash Algorithm for Key Obfuscation",
                            "description": "Specify the hash algorithm to obfuscate keys before reading data from or writing data to the eXtreme Scale data grid."
                        }
                    ]
                },
                "description": "The configuration of an eXtreme Scale Grid defines the connection details to an eXtreme Scale grid in an eXtreme Scale collective. To define this configuration, you must define the eXtreme Scale collective, the grid name, and the user and password for the user account who connects to the eXtreme Scale. If you need to secure connections to eXtreme Scale, you must assign a TLS Proxy Profile."
            },
            "_links": {
                "doc": {
                    "href": "/mgmt/docs/metadata/WXSGrid"
                },
                "self": {
                    "href": "/mgmt/metadata/default/WXSGrid"
                }
            }
        }
    }
}