        "metadata": {
            "object": {
                "licensed-feature": "IDG",
                "name": "Domain",
                "status-bindings": {
                    "status": {
                        "binding": {
                            "property": {
                                "name": "Domain",
                                "select": "dmObjectName"
                            }
                        },
                        "name": "DomainSummary",
                        "display": {
                            "property": {
                                "name": "Quiesce",
                                "display": "Quiesce state"
                            }
                        }
                    }
                },
                "property-group": [
                    {
                        "member": [
                            {
                                "name": "mAdminState"
                            },
                            {
                                "name": "UserSummary"
                            }
                        ],
                        "name": "summary"
                    },
                    {
                        "member": [
                            {
                                "name": "MaxChkpoints"
                            },
                            {
                                "name": "ConfigMode"
                            },
                            {
                                "name": "ImportURL"
                            },
                            {
                                "name": "ImportFormat"
                            },
                            {
                                "name": "DeploymentPolicy"
                            },
                            {
                                "name": "DeploymentPolicyParameters"
                            },
                            {
                                "name": "LocalIPRewrite"
                            },
                            {
                                "name": "ConfigPermissionsMode"
                            },
                            {
                                "name": "ConfigPermissionsProfile"
                            }
                        ],
                        "name": "mode",
                        "display": "Configuration"
                    }
                ],
