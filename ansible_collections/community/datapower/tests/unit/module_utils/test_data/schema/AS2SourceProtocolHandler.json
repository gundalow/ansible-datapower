                            },
                            {
                                "name": "AllowedFeatures"
                            },
                            {
                                "name": "PersistentConnections"
                            },
                            {
                                "name": "AllowCompression"
                            }
                        ],
                        "name": "main",
                        "display": "Main"
                    },
                    {
                        "member": [
                            {
                                "name": "MaxURLLen"
                            },
                            {
                                "name": "MaxTotalHdrLen"
                            },
                            {
                                "name": "MaxHdrCount"
                            },
                            {
                                "name": "MaxNameHdrLen"
                            },
                            {
                                "name": "MaxValueHdrLen"
                            },
                            {
                                "name": "MaxQueryStringLen"
                            },
                            {
                                "name": "CredentialCharset"
                            },
                            {
                                "name": "ACL"
                            }
                        ],
                        "name": "advanced",
                        "display": "Advanced"
                    }
                ],
                "uri": "protocol/as2",
                "actions": {
                    "action": [
                        {
                            "name": "ServiceQuiesce",
                            "parameters": {
                                "parameter": [
                                    {
                                        "name": "type",
                                        "select": "dmObjectClass"
                                    },
                                    {
                                        "name": "name",
                                        "select": "dmObjectName"
                                    },
                                    {
                                        "name": "timeout"
                                    }
                                ]
                            }
                        },
                        {
                            "name": "ServiceUnquiesce",
                            "parameters": {
                                "parameter": [
                                    {
                                        "name": "type",
                                        "select": "dmObjectClass"
                                    },
                                    {
                                        "name": "name",
                                        "select": "dmObjectName"
                                    }
                                ]
                            }
                        }
                    ]
                },
                "summary": "AS2 Handler",
                "services": {
                    "service": "B2BGateway"
                },
                "cmd-group": "mpgw",
                "cli-alias": "source-as2",
                "properties": {
                    "property": [
                        {
                            "name": "mAdminState",
                            "default": "enabled",
                            "type": {
                                "href": "/mgmt/types/default/dmAdminState"
                            },
                            "label": "Enable administrative state",
                            "summary": "Set the administrative state of this configuration.",
                            "cli-alias": "admin-state",
                            "display": "Administrative state",
                            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>",
                            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
                        },
                        {
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "display": "Comments",
                            "cli-alias": "summary",
                            "name": "UserSummary",
                            "summary": "Brief summary for user annotation."
                        },
                        {
                            "name": "LocalAddress",
                            "default": "0.0.0.0",
                            "type": {
                                "href": "/mgmt/types/default/dmLocalIPHostAddress"
                            },
                            "required": "true",
                            "summary": "Local IP address",
                            "cli-alias": "local-address",
                            "display": "Local IP Address",
                            "description": "The address on which the service listens. The default of 0 indicates that the service is active on all addresses. Click Select Alias to use an alias for this value. Local host aliases help to ease migration tasks between machines. See Local Host Alias under Networking for more."
                        },
                        {
                            "name": "LocalPort",
                            "default": 80,
                            "cli-alias": "port",
                            "required": "true",
                            "maximum": "0xFFFF",
                            "summary": "The local TCP port number that supports server operations",
                            "listener": "LocalAddress",
                            "minimum": 1,
                            "type": {
                                "href": "/mgmt/types/default/dmIPPort"
                            },
                            "display": "Port Number",
                            "description": "An integer (within the range 1 through 65535, with a default of 80) that specifies the port monitored by the HTTP service."
                        },
                        {
                            "name": "HTTPVersion",
                            "default": "HTTP/1.1",
                            "type": {
                                "href": "/mgmt/types/default/dmHTTPVersion"
                            },
                            "summary": "Front side HTTP version",
                            "cli-alias": "http-client-version",
                            "display": "HTTP Version to Client",
                            "description": "Select the HTTP version to use on the front (client) side connection. The default is HTTP 1.1."
                        },
                        {
                            "name": "AllowedFeatures",
                            "default": "0x0617",
                            "type": {
                                "href": "/mgmt/types/default/dmSourceHTTPFeatureType"
                            },
                            "summary": "Request methods and versions must be of only the allowed types",
                            "cli-alias": "allowed-features",
                            "display": "Allowed Methods and Versions",
                            "description": "Specifies the allowed methods and versions for incoming HTTP requests."
                        },
                        {
                            "name": "PersistentConnections",
                            "default": "on",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Enable/Disable persistent connections on front side",
                            "cli-alias": "persistent-connections",
                            "display": "Persistent Connections",
                            "description": "Toggle to enable or disable persistent connections where appropriate on front side."
                        },
                        {
                            "name": "AllowCompression",
                            "default": "off",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Enable/Disable GZIP compression",
                            "cli-alias": "compression",
                            "display": "Compression",
                            "description": "Toggle to enable or disable the GZIP compression functionality on front side."
                        },
                        {
                            "name": "MaxURLLen",
                            "default": 16384,
                            "cli-alias": "max-url-len",
                            "maximum": 128000,
                            "summary": "The longest incoming URL that will be accepted",
                            "minimum": 1,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Maximum Allowed URL Length",
                            "description": "The longest incoming URL that will be accepted in bytes. The length includes any query string or fragment identifier."
                        },
                        {
                            "name": "MaxTotalHdrLen",
                            "default": 128000,
                            "cli-alias": "max-total-header-len",
                            "maximum": 128000,
                            "summary": "Maximum Aggregate Size of Incoming Headers",
                            "minimum": 5,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Maximum Allowed Total Header Length",
                            "description": "The maximum aggregate byte size of incoming HTTP headers."
                        },
                        {
                            "name": "MaxHdrCount",
                            "default": 0,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt16"
                            },
                            "summary": "Maximum Number of HTTP Request Headers Allowed",
                            "cli-alias": "max-header-count",
                            "display": "Maximum Number of HTTP Request Headers Allowed",
                            "description": "The maximum number of headers allowed in the request message. 0 means unlimited."
                        },
                        {
                            "name": "MaxNameHdrLen",
                            "default": 0,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt16"
                            },
                            "summary": "Maximum Allowed Length of HTTP Header Name",
                            "cli-alias": "max-header-name-len",
                            "display": "Maximum Allowed Length of HTTP Header Name",
                            "description": "Each HTTP Header is expressed as a name and value pair. This property specifies the maximum length of the name portion of that header. 0 means unlimited."
                        },
                        {
                            "name": "MaxValueHdrLen",
                            "default": 0,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt16"
                            },
                            "summary": "Maximum Allowed Length of HTTP Header Value",
                            "cli-alias": "max-header-value-len",
                            "display": "Maximum Allowed Length of HTTP Header Value",
                            "description": "Each HTTP Header is expressed as a name and value pair. This property specifies the maximum length of the value portion of that header. 0 means unlimited."
                        },
                        {
                            "name": "MaxQueryStringLen",
                            "default": 0,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt16"
                            },
                            "summary": "Maximum Allowed Length of HTTP Query String",
                            "cli-alias": "max-querystring-len",
                            "display": "Maximum Allowed Length of HTTP Query String",
                            "description": "The portion of the URL after a ? is the query string. This property specifies its maximum length in bytes. 0 means unlimited."
                        },
                        {
                            "name": "SSLProxy",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/SSLProxyProfile"
                                }
                            },
                            "summary": "The TLS proxy profile is deprecated. Use an TLS server profile or TLS SNI server profile.",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "SSLServerConfigType",
                                    "evaluation": "property-does-not-equal",
                                    "value": "proxy"
                                }
                            },
                            "cli-alias": "ssl",
                            "display": "TLS proxy profile (deprecated)",
                            "description": "Select an existing TLS proxy profile from the list to assign the TLS proxy profile to this handler. Click the + button to create a new TLS proxy profile and assign that TLS proxy profile to the handler."
                        },
                        {
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/AccessControlList"
                                }
                            },
