        "metadata": {
            "object": {
                "licensed-feature": "B2B",
                "name": "AS2SourceProtocolHandler",
                "status-bindings": {
                    "status": {
                        "binding": {
                            "property": {
                                "name": "AS2SourceProtocolHandler",
                                "select": "dmObjectName"
                            }
                        },
                        "name": "AS2SourceProtocolHandlerSummary",
                        "display": {
                            "property": {
                                "name": "Quiesce",
                                "display": "Quiesce State"
                            }
                        }
                    }
                },
                "display": "AS2 Handler",
                "property-group": [
                    {
                        "member": [
                            {
                                "name": "mAdminState"
                            },
                            {
                                "name": "UserSummary"
                            },
                            {
                                "name": "LocalAddress"
                            },
                            {
