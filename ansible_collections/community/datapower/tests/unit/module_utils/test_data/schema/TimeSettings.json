        "metadata": {
            "object": {
                "name": "TimeSettings",
                "display": "Time Settings",
                "uri": "system/timezone",
                "summary": "Set the local time zone for the appliance.",
                "cmd-group": "system",
                "cli-alias": "timezone",
                "properties": {
                    "property": [
                        {
                            "name": "mAdminState",
                            "default": "enabled",
                            "type": {
                                "href": "/mgmt/types/default/dmAdminState"
                            },
                            "label": "Enable administrative state",
                            "summary": "Set the administrative state of this configuration.",
                            "cli-alias": "admin-state",
                            "display": "Administrative state",
                            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>",
                            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
                        },
                        {
                            "name": "LocalTimeZone",
                            "default": "EST-5EDT",
                            "type": {
                                "href": "/mgmt/types/default/dmTimeZone"
                            },
                            "required": "true",
                            "summary": "Set the local time zone.",
                            "cli-alias": "name",
                            "display": "Local time zone",
                            "description": "The time zone to use in management interfaces. The default value is EST-5EDT."
                        },
                        {
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "name": "CustomTZName",
                            "default": "STD",
                            "required-when": {
                                "condition": {
                                    "property-name": "LocalTimeZone",
                                    "evaluation": "property-equals",
                                    "value": "Custom"
                                }
                            },
                            "summary": "Enter the symbolic name for the custom time zone.",
                            "cli-alias": "custom",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-true"
                                }
                            },
                            "valid-regexp": "^[a-zA-Z]{3,}$",
                            "display": "Name",
                            "hoverhelp": "Enter the symbolic name for the custom time zone. The name must be three or more alphabetic characters. If you use any other characters, the time zone becomes UTC.",
                            "description": "The symbolic name for the custom time zone. This name is appended to local times. The name must be three or more alphabetic characters. If you use any other characters, the time zone becomes UTC."
                        },
                        {
                            "name": "UTCDirection",
                            "default": "West",
                            "type": {
                                "href": "/mgmt/types/default/dmTimeDirection"
                            },
                            "summary": "Set the direction relative to UTC for the custom time zone.",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "LocalTimeZone",
                                    "evaluation": "property-does-not-equal",
                                    "value": "Custom"
                                }
                            },
                            "cli-alias": "direction",
                            "display": "Direction from UTC",
                            "hoverhelp": "Set the direction relative to UTC for the custom time zone. Asia is east. North America is west.",
                            "description": "The direction relative to UTC for the custom time zone. Asia is east. North America is west. The default value is East."
                        },
                        {
                            "name": "OffsetHours",
                            "default": 0,
                            "cli-alias": "offset-hours",
                            "maximum": 12,
                            "summary": "Enter the number of hours the custom time zone is from UTC.",
                            "minimum": 0,
                            "ignored-when": {
                                "condition": {
                                    "property-name": "LocalTimeZone",
                                    "evaluation": "property-does-not-equal",
                                    "value": "Custom"
                                }
                            },
                            "units": "Hours",
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Hours from UTC",
                            "hoverhelp": "Enter the number of hours the custom time zone is from UTC. If 2 hours and 30 minutes from UTC, enter 2.",
                            "description": "The number of hours the custom time zone is from UTC. If 2 hours and 30 minutes from UTC, enter 2."
                        },
                        {
                            "name": "OffsetMinutes",
                            "default": 0,
                            "cli-alias": "offset-minutes",
                            "maximum": 59,
                            "summary": "Enter the number of minutes the custom time zone is from UTC.",
                            "minimum": 0,
                            "ignored-when": {
                                "condition": {
                                    "property-name": "LocalTimeZone",
                                    "evaluation": "property-does-not-equal",
                                    "value": "Custom"
                                }
                            },
                            "units": "Minutes",
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Minutes from UTC",
                            "hoverhelp": "Enter the number of minutes the time zone is from UTC. If 2 hours and 30 minutes from UTC, enter 30.",
                            "description": "The number of minutes the time zone is from UTC. If 2 hours and 30 minutes from UTC, enter 30."
                        },
                        {
                            "name": "DaylightOffsetHours",
                            "default": 1,
                            "cli-alias": "daylight-offset",
                            "maximum": 12,
                            "summary": "Enter the offset hours from DST.",
                            "minimum": 0,
                            "ignored-when": {
                                "condition": {
                                    "property-name": "LocalTimeZone",
                                    "evaluation": "property-does-not-equal",
                                    "value": "Custom"
                                }
                            },
                            "units": "Hours",
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Daylight savings time (DST) offset",
                            "description": "The offset in hours when the custom time zone observes DST. Generally, the offset is 1 hour. The default value is 1."
                        },
                        {
                            "name": "TZNameDST",
                            "default": "DST",
                            "required-when": {
                                "condition": {
                                    "evaluation": "logical-and",
                                    "condition": [
                                        {
                                            "property-name": "LocalTimeZone",
                                            "evaluation": "property-equals",
                                            "value": "Custom"
                                        },
                                        {
                                            "property-name": "DaylightOffsetHours",
                                            "evaluation": "property-does-not-equal",
                                            "value": 0
                                        }
                                    ]
                                }
                            },
                            "summary": "Enter the symbolic name for the custom time zone during DST.",
                            "cli-alias": "daylight-name",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-or",
                                    "condition": [
                                        {
                                            "property-name": "LocalTimeZone",
                                            "evaluation": "property-does-not-equal",
                                            "value": "Custom"
                                        },
                                        {
                                            "property-name": "DaylightOffsetHours",
                                            "evaluation": "property-equals",
                                            "value": 0
                                        }
                                    ]
                                }
                            },
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "display": "DST name",
                            "hoverhelp": "Enter the symbolic name for the custom time zone during DST. The name must be three or more alphabetic characters. If you use any other characters, the time zone becomes UTC.",
                            "description": "The symbolic name for the custom time zone during DST. This name is appended to local times. The name must be three or more alphabetic characters. If you use any other characters, the time zone becomes UTC."
                        },
                        {
                            "name": "DaylightStartMonth",
                            "default": "March",
                            "type": {
                                "href": "/mgmt/types/default/dmMonth"
                            },
                            "summary": "Set the month when DST starts for the custom time zone.",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-or",
                                    "condition": [
                                        {
                                            "property-name": "LocalTimeZone",
                                            "evaluation": "property-does-not-equal",
                                            "value": "Custom"
                                        },
                                        {
                                            "property-name": "DaylightOffsetHours",
                                            "evaluation": "property-equals",
                                            "value": 0
                                        }
                                    ]
                                }
                            },
                            "cli-alias": "daylight-start-month",
                            "display": "DST start month",
                            "description": "The month when DST starts for the custom time zone. The default value is March."
                        },
                        {
                            "name": "DaylightStartWeek",
                            "default": 2,
                            "cli-alias": "daylight-start-week",
                            "maximum": 5,
                            "summary": "Enter the instance of the day in the month when DST starts for the custom time zone.",
                            "minimum": 1,
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-or",
                                    "condition": [
                                        {
                                            "property-name": "LocalTimeZone",
                                            "evaluation": "property-does-not-equal",
                                            "value": "Custom"
                                        },
                                        {
                                            "property-name": "DaylightOffsetHours",
                                            "evaluation": "property-equals",
                                            "value": 0
                                        }
                                    ]
                                }
                            },
                            "units": "Instance",
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "DST start day instance",
                            "hoverhelp": "Enter the instance of the day in the month when DST starts for the custom time zone. If DST starts on the second Sunday in the month, enter 2.",
                            "description": "The instance of the day in the month when DST starts for the custom time zone. If DST starts on the second Sunday in the month, enter 2."
                        },
                        {
                            "name": "DaylightStartDay",
                            "default": "Sunday",
                            "type": {
                                "href": "/mgmt/types/default/dmWeekday"
                            },
                            "summary": "Set the day of the week when DST starts for the custom time zone.",
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-or",
                                    "condition": [
                                        {
                                            "property-name": "LocalTimeZone",
                                            "evaluation": "property-does-not-equal",
                                            "value": "Custom"
                                        },
                                        {
                                            "property-name": "DaylightOffsetHours",
                                            "evaluation": "property-equals",
                                            "value": 0
                                        }
                                    ]
                                }
                            },
                            "cli-alias": "daylight-start-day",
                            "display": "DST start day",
                            "description": "The day of the week when DST starts for the custom time zone. The default value is Sunday."
                        },
                        {
                            "name": "DaylightStartTimeHours",
                            "default": 2,
                            "cli-alias": "daylight-start-hours",
                            "maximum": 23,
                            "summary": "Enter the hour when DST starts.",
                            "minimum": 0,
                            "ignored-when": {
                                "condition": {
                                    "evaluation": "logical-or",
                                    "condition": [
                                        {
                                            "property-name": "LocalTimeZone",
                                            "evaluation": "property-does-not-equal",
                                            "value": "Custom"
                                        },
                                        {
                                            "property-name": "DaylightOffsetHours",
                                            "evaluation": "property-equals",
                                            "value": 0
                                        }
                                    ]
                                }
                            },
                            "units": "Hours",
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "DST start hour",
                            "hoverhelp": "Enter the hour when DST starts for the custom time zone. If the start boundary is 2:30 AM, enter 2.",
                            "description": "The hour when DST starts for the custom time zone. If the start boundary is 2:30 AM, enter 2."
                        },
                        {
                            "name": "DaylightStartTimeMinutes",
                            "default": 0,
                            "cli-alias": "daylight-start-minutes",
                            "maximum": 59,
                            "summary": "Enter the minute when DST starts for the custom time zone.",
                            "minimum": 0,
                            "ignored-when": {
                                "condition": {
