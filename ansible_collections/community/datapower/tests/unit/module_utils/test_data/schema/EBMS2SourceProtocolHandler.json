        "metadata": {
            "object": {
                "licensed-feature": "B2B",
                "name": "EBMS2SourceProtocolHandler",
                "status-bindings": {
                    "status": {
                        "binding": {
                            "property": {
                                "name": "EBMS2SourceProtocolHandler",
                                "select": "dmObjectName"
                            }
                        },
                        "name": "EBMS2SourceProtocolHandlerSummary",
                        "display": {
                            "property": {
                                "name": "Quiesce",
                                "display": "Quiesce State"
                            }
                        }
                    }
                },
                "display": "ebMS2 Handler",
                "property-group": [
                    {
                        "member": [
                            {
                                "name": "mAdminState"
                            },
                            {
                                "name": "UserSummary"
                            },
                            {
                                "name": "LocalAddress"
                            },
