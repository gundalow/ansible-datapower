                            },
                            {
                                "name": "MaxNameHdrLen"
                            },
                            {
                                "name": "MaxValueHdrLen"
                            },
                            {
                                "name": "MaxQueryStringLen"
                            },
                            {
                                "name": "SSLServerConfigType"
                            },
                            {
                                "name": "SSLProxy"
                            },
                            {
                                "name": "SSLServer"
                            },
                            {
                                "name": "SSLSNIServer"
                            },
                            {
                                "name": "ACL"
                            },
                            {
                                "name": "AAAPolicy"
                            },
                            {
                                "name": "CredentialCharset"
                            }
                        ],
                        "name": "main",
                        "display": "Main"
                    }
                ],
                "uri": "protocol/ebms2",
                "actions": {
                    "action": [
                        {
                            "name": "ServiceQuiesce",
                            "parameters": {
                                "parameter": [
                                    {
                                        "name": "type",
                                        "select": "dmObjectClass"
                                    },
                                    {
                                        "name": "name",
                                        "select": "dmObjectName"
                                    },
                                    {
                                        "name": "timeout"
                                    }
                                ]
                            }
                        },
                        {
                            "name": "ServiceUnquiesce",
                            "parameters": {
                                "parameter": [
                                    {
                                        "name": "type",
                                        "select": "dmObjectClass"
                                    },
                                    {
                                        "name": "name",
                                        "select": "dmObjectName"
                                    }
                                ]
                            }
                        }
                    ]
                },
                "summary": "ebMS2 Handler",
                "services": {
                    "service": "B2BGateway"
                },
                "cmd-group": "mpgw",
                "cli-alias": "source-ebms2",
                "properties": {
                    "property": [
                        {
                            "name": "mAdminState",
                            "default": "enabled",
                            "type": {
                                "href": "/mgmt/types/default/dmAdminState"
                            },
                            "label": "Enable administrative state",
                            "summary": "Set the administrative state of this configuration.",
                            "cli-alias": "admin-state",
                            "display": "Administrative state",
                            "hoverhelp": "&lt;p>Set the administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set the check box.&lt;/li>&lt;li>To make inactive, clear the check box.&lt;/li>&lt;/ul>",
                            "description": "&lt;p>The administrative state of the configuration.&lt;/p>&lt;ul>&lt;li>To make active, set to enabled.&lt;/li>&lt;li>To make inactive, set to disabled.&lt;/li>&lt;/ul>"
                        },
                        {
                            "type": {
                                "href": "/mgmt/types/default/dmString"
                            },
                            "display": "Comments",
                            "cli-alias": "summary",
                            "name": "UserSummary",
                            "summary": "Brief summary for user annotation."
                        },
                        {
                            "name": "LocalAddress",
                            "default": "0.0.0.0",
                            "type": {
                                "href": "/mgmt/types/default/dmLocalIPHostAddress"
                            },
                            "required": "true",
                            "summary": "Local IP address",
                            "cli-alias": "local-address",
                            "display": "Local IP address",
                            "description": "The address on which the service listens. The default value of 0.0.0.0 indicates that the service is active on all addresses. Local host aliases help to ease migration tasks between machines."
                        },
                        {
                            "name": "LocalPort",
                            "default": 80,
                            "cli-alias": "port",
                            "required": "true",
                            "maximum": "0xFFFF",
                            "summary": "The local TCP port number that supports server operations",
                            "listener": "LocalAddress",
                            "minimum": 1,
                            "type": {
                                "href": "/mgmt/types/default/dmIPPort"
                            },
                            "display": "Port",
                            "description": "Specifies the port monitored by the HTTP service. Enter a value in the range 1 - 65535. The default value is 80."
                        },
                        {
                            "name": "HTTPVersion",
                            "default": "HTTP/1.1",
                            "type": {
                                "href": "/mgmt/types/default/dmHTTPVersion"
                            },
                            "summary": "Front side HTTP version",
                            "cli-alias": "http-client-version",
                            "display": "HTTP version to client",
                            "description": "Select the HTTP version to use on the front (client) side connection. The default is HTTP 1.1."
                        },
                        {
                            "name": "PersistentConnections",
                            "default": "on",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Whether to enable persistent connections",
                            "cli-alias": "persistent-connections",
                            "display": "Negotiate persistent connections",
                            "description": "Controls whether to enable persistent connections on the front side. The default is enabled."
                        },
                        {
                            "name": "AllowCompression",
                            "default": "off",
                            "type": {
                                "href": "/mgmt/types/default/dmToggle"
                            },
                            "summary": "Whether to enable GZIP compression",
                            "cli-alias": "compression",
                            "display": "Enable compression",
                            "description": "Controls whether to enable GZIP compression on the front side. The default is disabled."
                        },
                        {
                            "name": "MaxURLLen",
                            "default": 16384,
                            "cli-alias": "max-url-len",
                            "maximum": 128000,
                            "summary": "Longest incoming URL that is accepted",
                            "minimum": 1,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Maximum URL length",
                            "description": "An integer in the range 1 to 128000 that specifies the longest incoming URL that is accepted in bytes. The length includes any query string or fragment identifier. The default value is 16384."
                        },
                        {
                            "name": "MaxTotalHdrLen",
                            "default": 128000,
                            "cli-alias": "max-total-header-len",
                            "maximum": 128000,
                            "summary": "Maximum aggregate size of incoming headers",
                            "minimum": 5,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt32"
                            },
                            "display": "Maximum total header length",
                            "description": "Specifies the maximum aggregate size of incoming HTTP headers in bytes. Enter a value in the range 5 - 128000. The default value is 128000."
                        },
                        {
                            "name": "MaxHdrCount",
                            "default": 0,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt16"
                            },
                            "summary": "Maximum number of HTTP request headers",
                            "cli-alias": "max-header-count",
                            "display": "Maximum number of headers",
                            "description": "Specifies the maximum number of headers that is allowed in the request message. The default value is 0, which indicates no limit."
                        },
                        {
                            "name": "MaxNameHdrLen",
                            "default": 0,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt16"
                            },
                            "summary": "Maximum length of HTTP header name",
                            "cli-alias": "max-header-name-len",
                            "display": "Maximum header name length",
                            "description": "Each HTTP Header is expressed as a name and value pair. This property specifies the maximum length of the name portion of the header. The default value is 0, which indicates no limit."
                        },
                        {
                            "name": "MaxValueHdrLen",
                            "default": 0,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt16"
                            },
                            "summary": "Maximum length of HTTP header value",
                            "cli-alias": "max-header-value-len",
                            "display": "Maximum header value length",
                            "description": "Each HTTP Header is expressed as a name and value pair. This property specifies the maximum length of the value portion of the header. The default value is 0, which indicates no limit."
                        },
                        {
                            "name": "MaxQueryStringLen",
                            "default": 0,
                            "type": {
                                "href": "/mgmt/types/default/dmUInt16"
                            },
                            "summary": "Maximum query string length",
                            "cli-alias": "max-querystring-len",
                            "display": "Maximum query string length",
                            "description": "The maximum length of the query string that is allowed in the client requests. The query string is the portion of the URL after the question mark (?) character. The default value is 0, which indicates no limit."
                        },
                        {
                            "name": "SSLProxy",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/SSLProxyProfile"
                                }
                            },
                            "summary": "The TLS proxy profile is deprecated. Use a TLS server profile or TLS SNI server profile.",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "SSLServerConfigType",
                                    "evaluation": "property-does-not-equal",
                                    "value": "proxy"
                                }
                            },
                            "cli-alias": "ssl",
                            "display": "TLS proxy profile (deprecated)",
                            "description": "Select an existing TLS proxy profile from the list to assign the TLS proxy profile to this handler. Click the + button to create a new TLS proxy profile and assign that TLS proxy profile to the handler."
                        },
                        {
                            "description": "The access control list (ACL) allows or denies access to this service based on the IP address of the client. When attached to a service, an ACL denies all access by default. To deny access to only selected addresses, first grant access to all addresses (allow 0.0.0.0) and then create deny entries for the desired hosts.",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/AccessControlList"
                                }
                            },
                            "summary": "Access control list that controls accesses to the service",
                            "cli-alias": "acl",
                            "display": "Access control list",
                            "name": "ACL"
                        },
                        {
                            "description": "Specifies the AAA Policy to use to authenticate the ebMS2 messages that are received on this handler.",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/AAAPolicy"
                                }
                            },
                            "summary": "AAA policy that authenticates the received messages",
                            "cli-alias": "aaa-policy",
                            "display": "AAA policy",
                            "name": "AAAPolicy"
                        },
                        {
                            "name": "CredentialCharset",
                            "default": "protocol",
                            "type": {
                                "href": "/mgmt/types/default/dmCredentialCharset"
                            },
                            "summary": "Character encoding of the original basic authentication values",
                            "cli-alias": "credential-charset",
                            "display": "Credential character set",
                            "description": "Specifies the character encoding of the original basic authentication values. Basic authentication credentials are combined and base64 encoded in the authorization header of the request. The DataPower Gateway transcodes the contents of the authorization header to UTF-8. The default value is Protocol, which indicates ISO-8859-1, Latin 1."
                        },
                        {
                            "name": "SSLServerConfigType",
                            "default": "proxy",
                            "type": {
                                "href": "/mgmt/types/default/dmSSLConfigType"
                            },
                            "summary": "Set the TLS profile type to secure connections between clients and the DataPower Gateway",
                            "cli-alias": "ssl-config-type",
                            "display": "TLS server type",
                            "description": "The TLS profile type to secure connections between clients and the DataPower Gateway."
                        },
                        {
                            "name": "SSLServer",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/SSLServerProfile"
                                }
                            },
                            "summary": "Set the TLS server profile to secure connections between clients and the DataPower Gateway",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "SSLServerConfigType",
                                    "evaluation": "property-does-not-equal",
                                    "value": "server"
                                }
                            },
                            "cli-alias": "ssl-server",
                            "display": "TLS server profile",
                            "description": "The TLS server profile to secure connections between clients and the DataPower Gateway."
                        },
                        {
                            "name": "SSLSNIServer",
                            "type": {
                                "href": "/mgmt/types/default/dmReference",
                                "reference-to": {
                                    "href": "/mgmt/metadata/default/SSLSNIServerProfile"
                                }
                            },
                            "summary": "Set the TLS SNI server profile to secure connections between clients and the DataPower Gateway",
                            "ignored-when": {
                                "condition": {
                                    "property-name": "SSLServerConfigType",
                                    "evaluation": "property-does-not-equal",
                                    "value": "sni"
                                }
                            },
                            "cli-alias": "ssl-sni-server",
                            "display": "TLS SNI server profile",
                            "description": "The TLS SNI server profile to secure connections between clients and the DataPower Gateway."
                        }
                    ]
                },
                "description": "The configuration for an ebMS2 handler."
            },
            "_links": {
                "doc": {
                    "href": "/mgmt/docs/metadata/EBMS2SourceProtocolHandler"
                },
                "self": {
                    "href": "/mgmt/metadata/default/EBMS2SourceProtocolHandler"
                }
            }
        }
    }
